<tr>
  <td><nobr><%= result.updated_at %></nobr></td>
  <td><%= result.item %></td>
  <td><%= result.condition %></td>
  <td><%= result.workerId %></td>
  <td>
    <% parsed = JSON.parse(result.data) %>
    <% entry = parsed ? parsed['entry']:nil %>
    <% if entry %>
        <%= link_to entry['category'], { action: 'view', id: result.id } %>
    <% else %>
        <%= link_to result.item, { action: 'view', id: result.id } %>
    <% end %>
  </td>
  <td>
    <% if entry %>
      <%= image_tag entry['url'] %>
    <% end %>
  </td>
  <td>
    <% if result.preview %>
        <% preview_url = result.preview.url %>
        <% if preview_url %>
            <%= image_tag preview_url %>
        <% end %>
    <% end %>
  </td>
  <td>
    <%= link_to 'Ã—',
                { controller: '/mturk', action: 'destroy_item', itemId: result.id },
                confirm: 'Are you sure?', method: :post,
                class: 'btn btn-danger scene-delete' %>
  </td>
</tr>